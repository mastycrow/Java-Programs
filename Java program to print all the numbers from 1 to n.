import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int a;
        Scanner m = new Scanner(System.in);
        a = m.nextInt();
        for (int i =1; i<=a ; i++){
            System.out.print(i+" ");
        }
    }
}
