import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }

        int ele = sc.nextInt();
        int pos = sc.nextInt();

        int[] narr = new int[n + 1];

        for (int i = 0; i < pos - 1; i++) {
            narr[i] = arr[i];
        }

        narr[pos - 1] = ele;

        for (int i = pos - 1; i < n; i++) {
            narr[i + 1] = arr[i];
        }

        for (int i = 0; i < narr.length; i++) {
            System.out.print(narr[i]);
            if (i != narr.length - 1) System.out.print(" ");
        }
        System.out.println();
    }
}
