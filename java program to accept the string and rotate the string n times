import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();     
        String str = sc.next();   
        
        int len = str.length();
        n = n % len;      
        String rotated = str.substring(n) + str.substring(0, n);
        
        System.out.println(rotated);
    }
}
