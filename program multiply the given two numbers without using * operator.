import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner m = new Scanner(System.in);
        int a= m.nextInt();
        int b= m.nextInt();
        int ans=0;
        for (int i=1; i<=b ; i++){
            ans = ans+a;
        }
        System.out.println(ans);    
    }
}
