import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner k = new Scanner(System.in);
        int m = k.nextInt();
        int n = k.nextInt();
        for (int i =1 ; i<=m ; i++){
            System.out.println(i +" * " + n + " = "+ i*n);
        }
    }
}
